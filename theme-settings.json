{
    "theme": {
        "color": {
            "primary": "#25252A",
            "secondary": "#2A2B30",
            "text-primary": {
                "dark": "#FFFFFF",
                "light": "#000000"
            },
            "text-secondary": "#808080",
            "fill": {
                "dark": "var(--color-primary)",
                "light": "#FFFFFF"
            },
            "fill-secondary": {
                "dark": "var(--color-secondary)",
                "light": "#FFFFFF"
            },
            "documentation-intro-fill": {
                "dark": "radial-gradient(circle at top, var(--color-secondary) 30%, #151516 100%)",
                "light": "radial-gradient(circle at top, #FFFFFF 30%, #F8F6FA 100%)"
            },
            "documentation-intro-eyebrow": "var(--color-text-secondary)",
            "documentation-intro-accent": "var(--color-text-secondary)",
            "documentation-intro-title": "var(--color-text-primary)",
            "documentation-intro-figure": "var(--color-text-primary)",

            "nav-solid-background": "var(--color-fill-primary)"
        }
    }
}
